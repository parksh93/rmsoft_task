<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.subscribe.task.repository.SubscribeRepository">
    <select id="findAllSub" resultType="com.subscribe.task.dto.subscribe.FindAllSubDTO">
        SELECT * FROM subscribe
    </select>

    <insert id="saveSub" parameterType="com.subscribe.task.dto.subscribe.SaveSubDTO">
        INSERT INTO subscribe VALUES (null, #{memberId}, #{personnel}, #{service}, #{storage}, #{startDate}, #{endDate})
    </insert>
</mapper>